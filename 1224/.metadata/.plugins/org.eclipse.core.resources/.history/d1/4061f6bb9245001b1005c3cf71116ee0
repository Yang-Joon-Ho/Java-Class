package com.javaEdu.ex;
import java.util.ArrayList;

public class StudentManageClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ArrayList<StudentClass> student = new ArrayList<StudentClass>();
		
		StudentClass st1 = new StudentClass("양준호", 26, "123");
		StudentClass st2 = new StudentClass("주커버그", 36, "124");
		StudentClass st3 = new StudentClass("리누스", 56, "125");
		
		st1.setSubject("OS");
		st2.setSubject("네트워크");
		st3.setSubject("자구");
		
		student.add(st1);
		student.add(st2);
		student.add(st3);
		
		st1.setName("호나우두");
				
		for(StudentClass s : student) {
			System.out.println(s.getName() + ", " + s.getAge() + ", "
					+ s.getNum() + ", " + s.getSubject());
		}
	}

}
